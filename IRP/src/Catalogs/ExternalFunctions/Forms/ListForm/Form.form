  <extInfo xsi:type="form:DynamicListExtInfo">
   <queryText>SELECT
	ExternalFunctions.Ref,
	ExternalFunctions.DeletionMark,
	ExternalFunctions.Parent,
	ExternalFunctions.IsFolder,
	ExternalFunctions.Description,
	ExternalFunctions.ExternalFunctionType,
	ExternalFunctions.Enable,
	ExternalFunctions.ExternalCode,
	ExternalFunctions.UseForSetDescription,
	ExternalFunctions.SafeModeIsOn,
	ExternalFunctions.RegExp,
	ExternalFunctions.isSchedulerSet,
	ExternalFunctions.JobSchedule,
	ExternalFunctions.DifficultLevel,
	JobQueueSliceLast.Period AS JobAddTime,
	JobQueueSliceLast.Start AS JobStartTime,
	JobQueueSliceLast.Status AS JobStatus,
	JobQueueSliceLast.Description AS JobInfo,
	JobQueueSliceLast.Finish AS JobFinishTime,
	JobQueueSliceLast.DifficultLevel AS JobDifficultLevel,
	JobQueueSliceLast.JobID,
	JobQueueSliceLast.UserMessages AS MessageFromJob
FROM
	Catalog.ExternalFunctions AS ExternalFunctions
	LEFT JOIN InformationRegister.JobQueue.SliceLast AS JobQueueSliceLast
		ON JobQueueSliceLast.Job = ExternalFunctions.Ref</queryText>
   <mainTable>Catalog.ExternalFunctions</mainTable>
