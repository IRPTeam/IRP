   <queryText>SELECT
	Table.Ref,
	Table.DeletionMark,
	Table.Parent,
	Table.Code,
	Table.OffBalance,
	Table.Predefined,
	Table.Quantity,
	Table.Currency,
	Table.LedgerTypeVariant,
	CASE
		WHEN Table.Type = VALUE(AccountType.Active)
			THEN "A"
		ELSE CASE
			WHEN Table.Type = VALUE(AccountType.Passive)
				THEN "P"
			ELSE "AP"
		END
	END AS Type,
	Table.Description_en AS Description,
	Table.Order AS Order,
	Table.NotUsedForRecords
FROM
