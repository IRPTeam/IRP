  <main>true</main>
  <extInfo xsi:type="form:DynamicListExtInfo">
   <queryText>SELECT
	Table.Ref AS Ref,
	Table.Code AS Code,
	Table.LedgerTypeVariant,
	Table.Description_en AS Description,
	CASE
		WHEN Table.Type = VALUE(AccountType.Active)
			THEN "A"
		ELSE CASE
			WHEN Table.Type = VALUE(AccountType.Passive)
				THEN "P"
			ELSE "AP"
		END
	END AS Type,
	Table.OffBalance AS OffBalance,
	Table.Currency AS Currency,
	Table.Quantity AS Quantity,
	Table.Parent AS Parent,
	Table.Order AS Order,
	ExtraDimension1.ExtraDimension AS ExtraDimension1,
	ExtraDimension2.ExtraDimension AS ExtraDimension2,
	ExtraDimension3.ExtraDimension AS ExtraDimension3,
	Table.NotUsedForRecords
FROM
	ChartOfAccounts.Basic AS Table
		LEFT JOIN (SELECT
			MasterExtraDimensions.ExtDimensionType.Description_en + CASE
				WHEN MasterExtraDimensions.TurnoversOnly
					THEN " (turn.)"
				ELSE ""
			END AS ExtraDimension,
			MasterExtraDimensions.Ref AS Ref
		FROM
			ChartOfAccounts.Basic.ExtDimensionTypes AS MasterExtraDimensions
		WHERE
			MasterExtraDimensions.LineNumber = 1) AS ExtraDimension1
		ON Table.Ref = ExtraDimension1.Ref
		LEFT JOIN (SELECT
			MasterExtraDimensions.ExtDimensionType.Description_en + CASE
				WHEN MasterExtraDimensions.TurnoversOnly
					THEN " (turn.)"
				ELSE ""
			END AS ExtraDimension,
			MasterExtraDimensions.Ref AS Ref
		FROM
			ChartOfAccounts.Basic.ExtDimensionTypes AS MasterExtraDimensions
		WHERE
			MasterExtraDimensions.LineNumber = 2) AS ExtraDimension2
		ON Table.Ref = ExtraDimension2.Ref
		LEFT JOIN (SELECT
			MasterExtraDimensions.ExtDimensionType.Description_en + CASE
				WHEN MasterExtraDimensions.TurnoversOnly
					THEN " (turn.)"
				ELSE ""
			END AS ExtraDimension,
			MasterExtraDimensions.Ref AS Ref
		FROM
			ChartOfAccounts.Basic.ExtDimensionTypes AS MasterExtraDimensions
		WHERE
			MasterExtraDimensions.LineNumber = 3) AS ExtraDimension3
		ON Table.Ref = ExtraDimension3.Ref</queryText>
