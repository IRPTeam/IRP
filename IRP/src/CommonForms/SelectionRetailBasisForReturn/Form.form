    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT
	R2050T_RetailSalesTurnovers.RetailSalesReceipt,
	SalesTable.Date AS SaleDate,
	SalesTable.RetailCustomer AS RetailCustomer,
	R2050T_RetailSalesTurnovers.ItemKey,
	SUM(R2050T_RetailSalesTurnovers.QuantityTurnover) AS Quantity,
	SUM(R2050T_RetailSalesTurnovers.AmountTurnover * CASE
		WHEN R2050T_RetailSalesTurnovers.QuantityTurnover &lt; 0
			THEN -1
		ELSE 1
	END) AS Amount
FROM
	AccumulationRegister.R2050T_RetailSales.Turnovers AS R2050T_RetailSalesTurnovers
		LEFT JOIN Document.RetailSalesReceipt AS SalesTable
		ON R2050T_RetailSalesTurnovers.RetailSalesReceipt = SalesTable.Ref
GROUP BY
	R2050T_RetailSalesTurnovers.RetailSalesReceipt,
	R2050T_RetailSalesTurnovers.ItemKey,
	SalesTable.Date,
	SalesTable.RetailCustomer
HAVING
	SUM(R2050T_RetailSalesTurnovers.QuantityTurnover) > 0</queryText>
