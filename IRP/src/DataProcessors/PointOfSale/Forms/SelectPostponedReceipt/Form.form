  <extInfo xsi:type="form:DynamicListExtInfo">
   <queryText>SELECT ALLOWED
	RetailSalesReceipt.Ref,
	RetailSalesReceipt.DataVersion,
	RetailSalesReceipt.Date,
	RetailSalesReceipt.DeletionMark,
	RetailSalesReceipt.Number,
	RetailSalesReceipt.Posted,
	RetailSalesReceipt.Company,
	RetailSalesReceipt.Currency,
	RetailSalesReceipt.RetailCustomer,
	RetailSalesReceipt.Workstation,
	RetailSalesReceipt.StatusType,
	RetailSalesReceipt.PointInTime,
	RetailSalesReceipt.Presentation,
	RetailSalesReceipt.ConsolidatedRetailSales,
	RetailSalesReceipt.Author,
	RetailSalesReceipt.Branch,
	RetailSalesReceipt.DocumentAmount,
	VALUE(Enum.SalesTransactionTypes.RetailSales) AS Type
FROM
	Document.RetailSalesReceipt AS RetailSalesReceipt
WHERE
	RetailSalesReceipt.StatusType IN (Value(Enum.RetailReceiptStatusTypes.Postponed),
		Value(Enum.RetailReceiptStatusTypes.PostponedWithReserve))
	AND RetailSalesReceipt.Branch = &amp;Branch
	AND NOT RetailSalesReceipt.DeletionMark

UNION ALL

SELECT
	RetailReturnReceipt.Ref,
	RetailReturnReceipt.DataVersion,
	RetailReturnReceipt.Date,
	RetailReturnReceipt.DeletionMark,
	RetailReturnReceipt.Number,
	RetailReturnReceipt.Posted,
	RetailReturnReceipt.Company,
	RetailReturnReceipt.Currency,
	RetailReturnReceipt.RetailCustomer,
	RetailReturnReceipt.Workstation,
	RetailReturnReceipt.StatusType,
	RetailReturnReceipt.PointInTime,
	RetailReturnReceipt.Presentation,
	RetailReturnReceipt.ConsolidatedRetailSales,
	RetailReturnReceipt.Author,
	RetailReturnReceipt.Branch,
	RetailReturnReceipt.DocumentAmount,
	VALUE(Enum.SalesReturnTransactionTypes.ReturnFromCustomer)
FROM
	Document.RetailReturnReceipt AS RetailReturnReceipt
WHERE
	RetailReturnReceipt.StatusType IN (Value(Enum.RetailReceiptStatusTypes.Postponed),
		Value(Enum.RetailReceiptStatusTypes.PostponedWithReserve))
	AND RetailReturnReceipt.Branch = &amp;Branch
	AND NOT RetailReturnReceipt.DeletionMark</queryText>
