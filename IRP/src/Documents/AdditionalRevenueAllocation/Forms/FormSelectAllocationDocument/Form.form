   <queryText>SELECT
	T6010S_BatchesInfo.Company,
	T6010S_BatchesInfo.Document AS Basis
FROM
	InformationRegister.T6010S_BatchesInfo AS T6010S_BatchesInfo
WHERE
	T6010S_BatchesInfo.Company = &amp;Company
	AND T6010S_BatchesInfo.Document REFS Document.PurchaseInvoice</queryText>
