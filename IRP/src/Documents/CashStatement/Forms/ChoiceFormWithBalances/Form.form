    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT
	CashStatement.Date,
	CashStatement.Number,
	CashStatement.Company,
	CashStatement.Branch,
	SUM(CashStatementPaymentList.Amount) AS Amount,
	SUM(R3021B_CashInTransitIncomingBalance.AmountBalance) AS AmountBalance,
	SUM(CashStatementPaymentList.Commission) AS Commission,
	SUM(R3021B_CashInTransitIncomingBalance.CommissionBalance) AS CommissionBalance,
	CashStatement.Ref
FROM
	Document.CashStatement AS CashStatement
		LEFT JOIN AccumulationRegister.R3021B_CashInTransitIncoming.Balance AS R3021B_CashInTransitIncomingBalance
		ON R3021B_CashInTransitIncomingBalance.Basis = CashStatement.Ref
		AND
			R3021B_CashInTransitIncomingBalance.CurrencyMovementType = VALUE(ChartOfCharacteristicTypes.CurrencyMovementType.SettlementCurrency)
		LEFT JOIN Document.CashStatement.PaymentList AS CashStatementPaymentList
		ON CashStatementPaymentList.Ref = CashStatement.Ref
		AND CashStatementPaymentList.PaymentType.Type = VALUE(Enum.PaymentTypes.Card)
WHERE
	CashStatement.Company = &amp;Company
	AND CashStatement.Branch = &amp;Branch
	AND CashStatement.CashAccount = &amp;Account
GROUP BY
	CashStatement.Ref,
	CashStatement.Company,
	CashStatement.Branch,
	CashStatement.BegOfPeriod,
	CashStatement.EndOfPeriod,
	CashStatement.Date</queryText>
      <mainTable>Document.CashStatement</mainTable>
