    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT
	RetailSalesReceipt.Ref,
	RetailSalesReceipt.Number,
	RetailSalesReceipt.Date,
	RetailSalesReceipt.Company,
	RetailSalesReceipt.Currency,
	RetailSalesReceipt.Author,
	RetailSalesReceipt.Branch,
	RetailSalesReceipt.DocumentAmount,
	CASE
		WHEN RetailSalesReceipt.Posted
			THEN 0
		WHEN RetailSalesReceipt.DeletionMark
			THEN 1
		ELSE 2
	END AS Icon
INTO Doc
FROM
	Document.RetailSalesReceipt AS RetailSalesReceipt
WHERE
	RetailSalesReceipt.ConsolidatedRetailSales = &amp;ConsolidatedRetailSales
	AND NOT RetailSalesReceipt.ConsolidatedRetailSales.Ref IS NULL

UNION ALL

SELECT
	RetailReturnReceipt.Ref,
	RetailReturnReceipt.Number,
	RetailReturnReceipt.Date,
	RetailReturnReceipt.Company,
	RetailReturnReceipt.Currency,
	RetailReturnReceipt.Author,
	RetailReturnReceipt.Branch,
	- RetailReturnReceipt.DocumentAmount,
	CASE
		WHEN RetailReturnReceipt.Posted
			THEN 0
		WHEN RetailReturnReceipt.DeletionMark
			THEN 1
		ELSE 2
	END AS Icon
FROM
	Document.RetailReturnReceipt AS RetailReturnReceipt
WHERE
	RetailReturnReceipt.ConsolidatedRetailSales = &amp;ConsolidatedRetailSales
	AND NOT RetailReturnReceipt.ConsolidatedRetailSales.Ref IS NULL
;

////////////////////////////////////////////////////////////////////////////////
SELECT
	Doc.Ref AS Document,
	Doc.Number,
	Doc.Date AS Date,
	Doc.Company,
	Doc.Currency,
	Doc.Author,
	Doc.Branch,
	Doc.DocumentAmount,
	Doc.Icon
FROM
	Doc AS Doc

ORDER BY
	Date</queryText>
