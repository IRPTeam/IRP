    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT
	R1021B_VendorsTransactionsBalance.Basis AS Invoice,
	R1021B_VendorsTransactionsBalance.Company AS Company,
	R1021B_VendorsTransactionsBalance.Branch AS Branch,
	R1021B_VendorsTransactionsBalance.Currency AS Currency,
	R1021B_VendorsTransactionsBalance.Partner AS Partner,
	R1021B_VendorsTransactionsBalance.LegalName AS LegalName,
	R1021B_VendorsTransactionsBalance.Agreement AS Agreement,
	R1021B_VendorsTransactionsBalance.Order AS InvoiceOrder,
	R1021B_VendorsTransactionsBalance.AmountBalance AS Debt
FROM
	AccumulationRegister.R1021B_VendorsTransactions.Balance(,
		CurrencyMovementType = VALUE(ChartOfCharacteristicTypes.CurrencyMovementType.SettlementCurrency)) AS
		R1021B_VendorsTransactionsBalance
WHERE
	NOT R1021B_VendorsTransactionsBalance.Basis.Ref IS NULL

UNION ALL

SELECT
	R2021B_CustomersTransactionsBalance.Basis,
	R2021B_CustomersTransactionsBalance.Company,
	R2021B_CustomersTransactionsBalance.Branch,
	R2021B_CustomersTransactionsBalance.Currency,
	R2021B_CustomersTransactionsBalance.Partner,
	R2021B_CustomersTransactionsBalance.LegalName,
	R2021B_CustomersTransactionsBalance.Agreement,
	R2021B_CustomersTransactionsBalance.Order,
	R2021B_CustomersTransactionsBalance.AmountBalance
FROM
	AccumulationRegister.R2021B_CustomersTransactions.Balance(,
		CurrencyMovementType = VALUE(ChartOfCharacteristicTypes.CurrencyMovementType.SettlementCurrency)) AS
		R2021B_CustomersTransactionsBalance
WHERE
	NOT R2021B_CustomersTransactionsBalance.Basis.Ref IS NULL</queryText>
