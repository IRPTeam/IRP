    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT ALLOWED
	R1020B_AdvancesToVendorsBalance.Basis AS Document,
	R1020B_AdvancesToVendorsBalance.Company AS Company,
	R1020B_AdvancesToVendorsBalance.Branch AS Branch,
	R1020B_AdvancesToVendorsBalance.Currency AS Currency,
	R1020B_AdvancesToVendorsBalance.Partner AS Partner,
	R1020B_AdvancesToVendorsBalance.LegalName AS LegalName,
	R1020B_AdvancesToVendorsBalance.Agreement AS Agreement,
	R1020B_AdvancesToVendorsBalance.Order AS InvoiceOrder,
	R1020B_AdvancesToVendorsBalance.AmountBalance AS Advance
FROM
	AccumulationRegister.R1020B_AdvancesToVendors.Balance(,
		CurrencyMovementType = VALUE(ChartOfCharacteristicTypes.CurrencyMovementType.SettlementCurrency)) AS
		R1020B_AdvancesToVendorsBalance
WHERE
	NOT R1020B_AdvancesToVendorsBalance.Basis.Ref IS NULL

UNION ALL

SELECT
	R2020B_AdvancesFromCustomersBalance.Basis,
	R2020B_AdvancesFromCustomersBalance.Company,
	R2020B_AdvancesFromCustomersBalance.Branch,
	R2020B_AdvancesFromCustomersBalance.Currency,
	R2020B_AdvancesFromCustomersBalance.Partner,
	R2020B_AdvancesFromCustomersBalance.LegalName,
	R2020B_AdvancesFromCustomersBalance.Agreement,
	R2020B_AdvancesFromCustomersBalance.Order,
	R2020B_AdvancesFromCustomersBalance.AmountBalance
FROM
	AccumulationRegister.R2020B_AdvancesFromCustomers.Balance(,
		CurrencyMovementType = VALUE(ChartOfCharacteristicTypes.CurrencyMovementType.SettlementCurrency)) AS
		R2020B_AdvancesFromCustomersBalance
WHERE
	NOT R2020B_AdvancesFromCustomersBalance.Basis.Ref IS NULL</queryText>
