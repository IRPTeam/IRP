  <extInfo xsi:type="form:DynamicListExtInfo">
   <queryText>SELECT
	MoneyTransfer.Ref,
	MoneyTransfer.Date,
	MoneyTransfer.Sender,
	MoneyTransfer.Receiver,
	MoneyTransfer.SendAmount,
	MoneyTransfer.SendCurrency,
	MoneyTransfer.ReceiveAmount,
	MoneyTransfer.ReceiveCurrency,
	MoneyTransfer.Company,
	MoneyTransfer.Author,
	MoneyTransfer.Description,
	MAX(JE.LedgerType) AS LedgerType,
	MAX(CASE
		WHEN NOT JE.Ref IS NULL
			THEN 0
		ELSE 1
	END) AS IsHaveJE,
	MoneyTransfer.Number
FROM
	Document.MoneyTransfer AS MoneyTransfer
		LEFT JOIN Document.JournalEntry AS JE
		ON MoneyTransfer.Ref = CAST(JE.Basis AS Document.MoneyTransfer)
		AND NOT JE.DeletionMark
GROUP BY
	MoneyTransfer.Ref,
	MoneyTransfer.Date,
	MoneyTransfer.Sender,
	MoneyTransfer.Receiver,
	MoneyTransfer.SendAmount,
	MoneyTransfer.SendCurrency,
	MoneyTransfer.ReceiveAmount,
	MoneyTransfer.ReceiveCurrency,
	MoneyTransfer.Company,
	MoneyTransfer.Author,
	MoneyTransfer.Description,
	MoneyTransfer.Number</queryText>
   <mainTable>Document.MoneyTransfer</mainTable>
