  <extInfo xsi:type="form:DynamicListExtInfo">
   <queryText>SELECT
	OutgoingMessage.Ref,
	OutgoingMessage.Date,
	OutgoingMessage.MailAccount,
	OutgoingMessage.Subject,
	OutgoingMessage.LetterRecipientsString AS Recipients,
	OutgoingMessage.Importance,
	OutgoingMessage.LetterSize,
	OutgoingMessage.HasAttachments,
	OutgoingMessage.NotifyDelivery,
	OutgoingMessage.NotifyReading,
	OutgoingMessage.BasisDocument,
	Max(ISNULL(MessagesQueueToSend.Sent, FALSE)) AS Sent,
	Max(ISNULL(MessagesQueueToSend.DateSent, DATETIME(1, 1, 1))) AS DateSent,
	MAX(NOT MessagesQueueToSend.Message IS NULL) AS ToSend
FROM
	Document.OutgoingMessage AS OutgoingMessage
		LEFT JOIN InformationRegister.MessagesQueueToSend AS MessagesQueueToSend
		ON OutgoingMessage.Ref = MessagesQueueToSend.Message
GROUP BY
	OutgoingMessage.Ref,
	OutgoingMessage.Date,
	OutgoingMessage.MailAccount,
	OutgoingMessage.Subject,
	OutgoingMessage.LetterRecipientsString,
	OutgoingMessage.Importance,
	OutgoingMessage.LetterSize,
	OutgoingMessage.HasAttachments,
	OutgoingMessage.NotifyDelivery,
	OutgoingMessage.NotifyReading,
	OutgoingMessage.BasisDocument</queryText>
   <mainTable>Document.OutgoingMessage</mainTable>
