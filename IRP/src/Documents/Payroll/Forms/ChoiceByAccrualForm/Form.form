  <extInfo xsi:type="form:DynamicListExtInfo">
   <queryText>SELECT
	R9510B_SalaryPaymentBalance.Employee AS Employee,
	R9510B_SalaryPaymentBalance.AmountBalance AS Amount
FROM
	AccumulationRegister.R9510B_SalaryPayment.Balance(&amp;Boundary, Company = &amp;Company
	AND Branch = &amp;Branch
	AND Currency = &amp;Currency
	AND CurrencyMovementType = VALUE(ChartOfCharacteristicTypes.CurrencyMovementType.SettlementCurrency)
	AND Employee NOT IN (&amp;ArrayOfEmployee)) AS R9510B_SalaryPaymentBalance</queryText>
   <mainTable>AccumulationRegister.R9510B_SalaryPayment.Balance</mainTable>
