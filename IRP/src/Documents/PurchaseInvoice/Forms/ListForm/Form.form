    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT
	PurchaseInvoiceItemList.Ref AS Ref,
	COUNT(DISTINCT PurchaseInvoiceItemList.Store) AS StoreCount,
	MAX(PurchaseInvoiceItemList.Store) AS Store,
	SUM(ISNULL(PurchaseInvoiceItemList.QuantityInBaseUnit, 0)) AS QuantityInBaseUnit
INTO ItemListTable
FROM
	Document.PurchaseInvoice.ItemList AS PurchaseInvoiceItemList
GROUP BY
	PurchaseInvoiceItemList.Ref

INDEX BY
	Ref
;

////////////////////////////////////////////////////////////////////////////////
 SELECT
	R1021B_VendorsTransactions.Recorder AS Basis,
	R1021B_VendorsTransactions.Amount,
	ISNULL(R1021B_VendorsTransactionsBalance.AmountBalance, 0) AS AmountBalance,
	CASE
		WHEN ISNULL(R1021B_VendorsTransactionsBalance.AmountBalance, 0) = 0
			THEN 0
		WHEN ISNULL(R1021B_VendorsTransactionsBalance.AmountBalance, 0) &lt; 0
			THEN 7
		WHEN ISNULL(R1021B_VendorsTransactionsBalance.AmountBalance, 0) &lt; R1021B_VendorsTransactions.Amount
			THEN 1
		ELSE 2
	END AS Status
INTO PaymentsTable
FROM
	AccumulationRegister.R1021B_VendorsTransactions AS R1021B_VendorsTransactions
		LEFT JOIN AccumulationRegister.R1021B_VendorsTransactions.Balance(,
			CurrencyMovementType = VALUE(ChartOfCharacteristicTypes.CurrencyMovementType.SettlementCurrency)) AS
			R1021B_VendorsTransactionsBalance
		ON R1021B_VendorsTransactionsBalance.Basis = R1021B_VendorsTransactions.Basis
		AND R1021B_VendorsTransactionsBalance.Basis = R1021B_VendorsTransactions.Recorder
		AND R1021B_VendorsTransactionsBalance.CurrencyMovementType = R1021B_VendorsTransactions.CurrencyMovementType
WHERE
	VALUETYPE(R1021B_VendorsTransactions.Recorder) = TYPE(Document.PurchaseInvoice)
	AND R1021B_VendorsTransactions.Agreement.ApArPostingDetail = VALUE(Enum.ApArPostingDetail.ByDocuments)
	AND R1021B_VendorsTransactions.Amount > 0
;

////////////////////////////////////////////////////////////////////////////////
 SELECT
	PurchaseInvoice.Ref,
	CASE
		WHEN ISNULL(R1031B_ReceiptInvoicingBalance.QuantityBalance, 0) = 0
			THEN VALUE(Enum.ShippingStatuses.Closed)
		WHEN ItemListTable.QuantityInBaseUnit = ISNULL(R1031B_ReceiptInvoicingBalance.QuantityBalance, 0)
			THEN VALUE(Enum.ShippingStatuses.Awaiting)
		WHEN ItemListTable.QuantityInBaseUnit &lt;> ISNULL(R1031B_ReceiptInvoicingBalance.QuantityBalance, 0)
			THEN VALUE(Enum.ShippingStatuses.Shipping)
	END AS Status,
	PurchaseInvoice.DeletionMark,
	PurchaseInvoice.Number,
	PurchaseInvoice.Date,
	PurchaseInvoice.Posted,
	PurchaseInvoice.Agreement,
	PurchaseInvoice.Company,
	PurchaseInvoice.Currency,
	PurchaseInvoice.TransactionType,
	PurchaseInvoice.DocDate,
	PurchaseInvoice.DocNumber,
	PurchaseInvoice.LegalName,
	PurchaseInvoice.Partner,
	PurchaseInvoice.PriceIncludeTax,
	PurchaseInvoice.Author,
	PurchaseInvoice.Branch,
	PurchaseInvoice.Description,
	PurchaseInvoice.DocumentAmount,
	CASE
		WHEN ItemListTable.StoreCount = 1
			THEN ItemListTable.Store
		ELSE VALUE(Catalog.Stores.EmptyRef)
	END AS Store,
	MAX(JE.LedgerType) AS LedgerType,
	MAX(CASE
		WHEN NOT JE.Ref IS NULL
			THEN 0
		ELSE 1
	END) AS IsHaveJE,
	MAX(ISNULL(Payments.Status, -1)) AS PaymentStatusIcon,
	MAX(CASE
		WHEN ISNULL(Payments.Status, -1) = 0
			THEN VALUE(Enum.PaymentStatuses.FullyPaid)
		WHEN ISNULL(Payments.Status, -1) = 7
			THEN VALUE(Enum.PaymentStatuses.Overpaid)
		WHEN ISNULL(Payments.Status, -1) = 1
			THEN VALUE(Enum.PaymentStatuses.PartiallyPaid)
		WHEN ISNULL(Payments.Status, -1) = 2
			THEN VALUE(Enum.PaymentStatuses.NotPaid)
		ELSE VALUE(Enum.PaymentStatuses.NotTracked)
	END) AS PaymentStatus,
	MAX(CASE
		WHEN AuditLock.Document IS NULL
			THEN -1
		ELSE 0
	END) AS AuditLockIcon,
	MAX(CASE
		WHEN AuditLock.Document IS NULL
			THEN Value(Enum.AuditLockActions.Unlock)
		ELSE Value(Enum.AuditLockActions.Lock)
	END) AS AuditLockStatus
{SELECT
	Store,
	LedgerType,
	IsHaveJE,
	PaymentStatusIcon,
	PaymentStatus}
FROM
	Document.PurchaseInvoice AS PurchaseInvoice
		LEFT JOIN ItemListTable AS ItemListTable
			LEFT JOIN PaymentsTable AS Payments
			ON ItemListTable.Ref = Payments.Basis
		ON PurchaseInvoice.Ref = ItemListTable.Ref
		LEFT JOIN AccumulationRegister.R1031B_ReceiptInvoicing.Balance AS R1031B_ReceiptInvoicingBalance
		ON PurchaseInvoice.Ref = CAST(R1031B_ReceiptInvoicingBalance.Basis AS Document.PurchaseInvoice)
		LEFT JOIN Document.JournalEntry AS JE
		ON PurchaseInvoice.Ref = CAST(JE.Basis AS Document.PurchaseInvoice)
		AND NOT JE.DeletionMark
		LEFT JOIN InformationRegister.AuditLock AS AuditLock
		ON Cast(AuditLock.Document AS Document.PurchaseInvoice) = PurchaseInvoice.Ref
GROUP BY
	PurchaseInvoice.Ref,
	PurchaseInvoice.DeletionMark,
	PurchaseInvoice.Number,
	PurchaseInvoice.Date,
	PurchaseInvoice.Posted,
	PurchaseInvoice.Agreement,
	PurchaseInvoice.Company,
	PurchaseInvoice.TransactionType,
	PurchaseInvoice.Currency,
	PurchaseInvoice.DocDate,
	PurchaseInvoice.DocNumber,
	PurchaseInvoice.LegalName,
	PurchaseInvoice.Partner,
	PurchaseInvoice.PriceIncludeTax,
	PurchaseInvoice.Author,
	PurchaseInvoice.Branch,
	PurchaseInvoice.Description,
	PurchaseInvoice.DocumentAmount,
	CASE
		WHEN ItemListTable.StoreCount = 1
			THEN ItemListTable.Store
		ELSE VALUE(Catalog.Stores.EmptyRef)
	END,
	R1031B_ReceiptInvoicingBalance.QuantityBalance,
	CASE
		WHEN ISNULL(R1031B_ReceiptInvoicingBalance.QuantityBalance, 0) = 0
			THEN VALUE(Enum.ShippingStatuses.Closed)
		WHEN ItemListTable.QuantityInBaseUnit = ISNULL(R1031B_ReceiptInvoicingBalance.QuantityBalance, 0)
			THEN VALUE(Enum.ShippingStatuses.Awaiting)
		WHEN ItemListTable.QuantityInBaseUnit &lt;> ISNULL(R1031B_ReceiptInvoicingBalance.QuantityBalance, 0)
			THEN VALUE(Enum.ShippingStatuses.Shipping)
	END</queryText>
