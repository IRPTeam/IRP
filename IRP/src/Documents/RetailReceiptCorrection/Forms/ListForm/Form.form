    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT
	RetailReceiptCorrection.Ref,
	RetailReceiptCorrection.DeletionMark,
	RetailReceiptCorrection.Number,
	RetailReceiptCorrection.Date,
	RetailReceiptCorrection.Posted,
	RetailReceiptCorrection.Partner,
	RetailReceiptCorrection.LegalName,
	RetailReceiptCorrection.Company,
	RetailReceiptCorrection.Currency,
	RetailReceiptCorrection.Manager,
	RetailReceiptCorrection.BasisDocument,
	RetailReceiptCorrection.Author,
	RetailReceiptCorrection.Branch,
	RetailReceiptCorrection.Description,
	RetailReceiptCorrection.DocumentAmount,
	RetailReceiptCorrection.RetailCustomer,
	RetailReceiptCorrection.ConsolidatedRetailSales,
	CASE
		WHEN COUNT(DISTINCT RetailReceiptCorrectionItemList.Store) = 1
			THEN MAX(RetailReceiptCorrectionItemList.Store)
		ELSE UNDEFINED
	END AS Store,
	RetailReceiptCorrection.StatusType,
	DocumentFiscalStatus.Status,
	DocumentFiscalStatus.CheckNumber
FROM
	Document.RetailReceiptCorrection AS RetailReceiptCorrection
		LEFT JOIN Document.RetailSalesReceipt.ItemList AS RetailReceiptCorrectionItemList
		ON RetailReceiptCorrectionItemList.Ref = RetailReceiptCorrection.Ref
		LEFT JOIN InformationRegister.DocumentFiscalStatus AS DocumentFiscalStatus
		ON DocumentFiscalStatus.Document = RetailReceiptCorrection.Ref
GROUP BY
	RetailReceiptCorrection.Ref,
	RetailReceiptCorrection.DeletionMark,
	RetailReceiptCorrection.Number,
	RetailReceiptCorrection.Date,
	RetailReceiptCorrection.Posted,
	RetailReceiptCorrection.Partner,
	RetailReceiptCorrection.LegalName,
	RetailReceiptCorrection.Company,
	RetailReceiptCorrection.Currency,
	RetailReceiptCorrection.Manager,
	RetailReceiptCorrection.BasisDocument,
	RetailReceiptCorrection.Author,
	RetailReceiptCorrection.Branch,
	RetailReceiptCorrection.Description,
	RetailReceiptCorrection.DocumentAmount,
	RetailReceiptCorrection.RetailCustomer,
	RetailReceiptCorrection.ConsolidatedRetailSales,
	RetailReceiptCorrection.StatusType,
	DocumentFiscalStatus.Status,
	DocumentFiscalStatus.CheckNumber</queryText>
