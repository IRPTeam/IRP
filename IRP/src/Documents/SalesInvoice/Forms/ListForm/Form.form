   <queryText>SELECT
	SalesInvoiceItemList.Ref AS Ref,
	COUNT(DISTINCT SalesInvoiceItemList.Store) AS StoreCount,
	MAX(SalesInvoiceItemList.Store) AS Store,
	SUM(ISNULL(SalesInvoiceItemList.QuantityInBaseUnit, 0)) AS QuantityInBaseUnit
INTO ItemListTable
FROM
	Document.SalesInvoice.ItemList AS SalesInvoiceItemList
GROUP BY
	SalesInvoiceItemList.Ref

INDEX BY
	Ref
;

////////////////////////////////////////////////////////////////////////////////
SELECT
	R2021B_CustomersTransactions.Recorder AS Basis,
	R2021B_CustomersTransactions.Amount,
	ISNULL(R2021B_CustomersTransactionsBalance.AmountBalance, 0) AS AmountBalance,
	CASE
		WHEN ISNULL(R2021B_CustomersTransactionsBalance.AmountBalance, 0) = 0
			THEN 0
		WHEN ISNULL(R2021B_CustomersTransactionsBalance.AmountBalance, 0) &lt; 0
			THEN 7
		WHEN ISNULL(R2021B_CustomersTransactionsBalance.AmountBalance, 0) &lt; R2021B_CustomersTransactions.Amount
			THEN 1
		ELSE 2
	END AS Status
INTO PaymentsTable
FROM
	AccumulationRegister.R2021B_CustomersTransactions AS R2021B_CustomersTransactions
		LEFT JOIN AccumulationRegister.R2021B_CustomersTransactions.Balance(,
			CurrencyMovementType = VALUE(ChartOfCharacteristicTypes.CurrencyMovementType.SettlementCurrency)) AS
			R2021B_CustomersTransactionsBalance
		ON R2021B_CustomersTransactionsBalance.Basis = R2021B_CustomersTransactions.Basis
		AND R2021B_CustomersTransactionsBalance.Basis = R2021B_CustomersTransactions.Recorder
		AND R2021B_CustomersTransactionsBalance.CurrencyMovementType = R2021B_CustomersTransactions.CurrencyMovementType
WHERE
	VALUETYPE(R2021B_CustomersTransactions.Recorder) = TYPE(Document.SalesInvoice)
	AND R2021B_CustomersTransactions.Agreement.ApArPostingDetail = VALUE(Enum.ApArPostingDetail.ByDocuments)
	AND R2021B_CustomersTransactions.Amount &gt; 0
;

////////////////////////////////////////////////////////////////////////////////
 SELECT
	SalesInvoice.Ref,
	CASE
		WHEN ISNULL(R2031B_ShipmentInvoicingBalance.QuantityBalance, 0) = 0
			THEN VALUE(Enum.ShippingStatuses.Closed)
		WHEN ItemListTable.QuantityInBaseUnit = ISNULL(R2031B_ShipmentInvoicingBalance.QuantityBalance, 0)
			THEN VALUE(Enum.ShippingStatuses.Awaiting)
		WHEN ItemListTable.QuantityInBaseUnit &lt;&gt; ISNULL(R2031B_ShipmentInvoicingBalance.QuantityBalance, 0)
			THEN VALUE(Enum.ShippingStatuses.Shipping)
	END AS Status,
	SalesInvoice.DeletionMark,
	SalesInvoice.Number,
	SalesInvoice.Date,
	SalesInvoice.Posted,
	SalesInvoice.Partner,
	SalesInvoice.LegalName,
	SalesInvoice.Company,
	SalesInvoice.TransactionType,
	SalesInvoice.Currency,
	SalesInvoice.Manager,
	SalesInvoice.BasisDocument,
	SalesInvoice.DocumentAmount,
	SalesInvoice.Author,
	CASE
		WHEN ItemListTable.StoreCount = 1
			THEN ItemListTable.Store
		ELSE VALUE(Catalog.Stores.EmptyRef)
	END AS Store,
	SalesInvoice.Description,
	MAX(JE.LedgerType) AS LedgerType,
	MAX(CASE
		WHEN NOT JE.Ref IS NULL
			THEN 0
		ELSE 1
	END) AS IsHaveJE,
	MAX(ISNULL(Payments.Status, -1)) AS PaymentStatusIcon,
	MAX(CASE
		WHEN ISNULL(Payments.Status, -1) = 0
			THEN VALUE(Enum.PaymentStatuses.FullyPaid)
		WHEN ISNULL(Payments.Status, -1) = 7
			THEN VALUE(Enum.PaymentStatuses.Overpaid)
		WHEN ISNULL(Payments.Status, -1) = 1
			THEN VALUE(Enum.PaymentStatuses.PartiallyPaid)
		WHEN ISNULL(Payments.Status, -1) = 2
			THEN VALUE(Enum.PaymentStatuses.NotPaid)
		ELSE VALUE(Enum.PaymentStatuses.NotTracked)
	END) AS PaymentStatus
{SELECT
	Store,
	LedgerType,
	IsHaveJE,
	PaymentStatusIcon,
	PaymentStatus}
FROM
	Document.SalesInvoice AS SalesInvoice
		LEFT JOIN ItemListTable AS ItemListTable
			LEFT JOIN PaymentsTable AS Payments
			ON ItemListTable.Ref = Payments.Basis
		ON ItemListTable.Ref = SalesInvoice.Ref
		LEFT JOIN AccumulationRegister.R2031B_ShipmentInvoicing.Balance AS R2031B_ShipmentInvoicingBalance
		ON SalesInvoice.Ref = CAST(R2031B_ShipmentInvoicingBalance.Basis AS Document.SalesInvoice)
		LEFT JOIN Document.JournalEntry AS JE
		ON SalesInvoice.Ref = CAST(JE.Basis AS Document.SalesInvoice)
		AND NOT JE.DeletionMark
GROUP BY
	SalesInvoice.Ref,
	SalesInvoice.DeletionMark,
	SalesInvoice.Number,
	SalesInvoice.Date,
	SalesInvoice.Posted,
	SalesInvoice.Partner,
	SalesInvoice.LegalName,
	SalesInvoice.Company,
	SalesInvoice.TransactionType,
	SalesInvoice.Currency,
	SalesInvoice.Manager,
	SalesInvoice.BasisDocument,
	SalesInvoice.DocumentAmount,
	SalesInvoice.Author,
	CASE
		WHEN ItemListTable.StoreCount = 1
			THEN ItemListTable.Store
		ELSE VALUE(Catalog.Stores.EmptyRef)
	END,
	R2031B_ShipmentInvoicingBalance.QuantityBalance,
	CASE
		WHEN ISNULL(R2031B_ShipmentInvoicingBalance.QuantityBalance, 0) = 0
			THEN VALUE(Enum.ShippingStatuses.Closed)
		WHEN ItemListTable.QuantityInBaseUnit = ISNULL(R2031B_ShipmentInvoicingBalance.QuantityBalance, 0)
			THEN VALUE(Enum.ShippingStatuses.Awaiting)
		WHEN ItemListTable.QuantityInBaseUnit &lt;&gt; ISNULL(R2031B_ShipmentInvoicingBalance.QuantityBalance, 0)
			THEN VALUE(Enum.ShippingStatuses.Shipping)
	END,
	SalesInvoice.Description</queryText>
