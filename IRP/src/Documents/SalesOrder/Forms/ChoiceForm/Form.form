  <extInfo xsi:type="form:DynamicListExtInfo">
   <queryText>SELECT 
       SalesOrder.Ref, 
       SalesOrder.DeletionMark, 
       SalesOrder.Number, 
       SalesOrder.Date, 
       SalesOrder.Posted, 
       SalesOrder.Partner, 
       SalesOrder.Company, 
       SalesOrder.Currency, 
       SalesOrder.DateOfShipment,  
       SalesOrder.ManagerSegment, 
       SalesOrder.Status, 
       SalesOrder.Agreement, 
       SalesOrder.PriceIncludeTax, 
       SalesOrder.DocumentAmount,
       SalesOrder.Author
 FROM 
       Document.SalesOrder
 AS 
       SalesOrder</queryText>
