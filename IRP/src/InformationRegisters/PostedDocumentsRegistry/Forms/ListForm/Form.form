    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT
	Reg.Document,
	Reg.Date,
	Reg.Number,
	Reg.CreateDate,
	Reg.ModifyDate,
	Reg.Author,
	Reg.Editor,
	Reg.ManualMovementsEdit
FROM
	InformationRegister.PostedDocumentsRegistry AS Reg
WHERE
	CASE
		WHEN &amp;Filter_DocumentType
			THEN VALUETYPE(Reg.Document) IN (&amp;ArrayOfDocumentTypes)
		ELSE TRUE
	END
	AND CASE
		WHEN &amp;Filter_StartDate
			THEN Reg.Date >= &amp;StartDate
		ELSE TRUE
	END
	AND CASE
		WHEN &amp;Filter_EndDate
			THEN Reg.Date &lt;= &amp;EndDate
		ELSE TRUE
	END</queryText>
