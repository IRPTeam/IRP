UNION

SELECT
	Doc.Ref,
	VALUETYPE(Doc.Ref)
FROM
	Document.Unbundling AS Doc</query>
	</dataSet>
	<calculatedField>
		<dataPath>Status</dataPath>
		<expression>StrConcat(AdditionalDocumentTableControl.CheckDocument(Ref, &amp;ErrorList), "
")</expression>
		<title xsi:type="v8:LocalStringType">
			<v8:item>
				<v8:lang>en</v8:lang>
				<v8:content>Status</v8:content>
			</v8:item>
			<v8:item>
				<v8:lang>ru</v8:lang>
				<v8:content>Статус</v8:content>
			</v8:item>
			<v8:item>
				<v8:lang>tr</v8:lang>
				<v8:content>Statü</v8:content>
			</v8:item>
		</title>
	</calculatedField>
	<parameter>
		<name>ErrorList</name>
		<title xsi:type="v8:LocalStringType">
			<v8:item>
				<v8:lang>en</v8:lang>
				<v8:content>Error list</v8:content>
			</v8:item>
		</title>
		<valueType>
			<v8:Type>xs:string</v8:Type>
			<v8:StringQualifiers>
				<v8:Length>0</v8:Length>
				<v8:AllowedLength>Variable</v8:AllowedLength>
			</v8:StringQualifiers>
		</valueType>
		<value xsi:nil="true"/>
		<useRestriction>false</useRestriction>
		<valueListAllowed>true</valueListAllowed>
	</parameter>
	<settingsVariant>
