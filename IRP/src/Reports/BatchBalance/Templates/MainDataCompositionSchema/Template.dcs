			<field xsi:type="DataSetFieldField">
				<dataPath>IsRelevance</dataPath>
				<field>IsRelevance</field>
				<title xsi:type="v8:LocalStringType">
					<v8:item>
						<v8:lang>ru</v8:lang>
						<v8:content>Актуальный</v8:content>
					</v8:item>
					<v8:item>
						<v8:lang>en</v8:lang>
						<v8:content>Is relevance</v8:content>
					</v8:item>
					<v8:item>
