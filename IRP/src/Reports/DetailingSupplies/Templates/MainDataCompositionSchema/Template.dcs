		<dataSource>DataSource1</dataSource>
		<query>SELECT
	DetailingSuppliesTurnovers.Company,
	DetailingSuppliesTurnovers.BusinessUnit,
	DetailingSuppliesTurnovers.Store,
	DetailingSuppliesTurnovers.PlanningPeriod,
	DetailingSuppliesTurnovers.ItemKey,
	// DEMAND
	DetailingSuppliesTurnovers.EntryDemandQuantityTurnover AS EntryDemandQuantity,
	DetailingSuppliesTurnovers.CorrectedDemandQuantityTurnover AS CorrectedDemandQuantity,
	DetailingSuppliesTurnovers.EntryDemandQuantityTurnover + DetailingSuppliesTurnovers.CorrectedDemandQuantityTurnover AS FinalDemandQuantity,
	
	// BALANCE
	ISNULL(StockBalanceBalance.QuantityBalance, 0) 
	+ ISNULL(StockBalanceBalance.QuantityBalance, 0) 
	- ISNULL(StockReservationBalance.QuantityBalance, 0) 
	- (DetailingSuppliesTurnovers.ReservedProduceQuantityTurnover -
		DetailingSuppliesTurnovers.WrittenOffProduceQuantityTurnover)
	- DetailingSuppliesTurnovers.ProducedProduceQuantityTurnover
	AS BalanceQuantity,
	
	ISNULL(StockReservationBalance.QuantityBalance, 0) AS FreeQuantity,
	
	ISNULL(StockBalanceBalance.QuantityBalance, 0) 
	- ISNULL(StockReservationBalance.QuantityBalance, 0) 
	- (DetailingSuppliesTurnovers.ReservedProduceQuantityTurnover -
		DetailingSuppliesTurnovers.WrittenOffProduceQuantityTurnover) AS ReservedQuantity,
	
	// PRODUCE
	DetailingSuppliesTurnovers.NeededProduceQuantityTurnover AS NeededProduceQuantity,
	DetailingSuppliesTurnovers.ProducedProduceQuantityTurnover AS ProducedProduceQuantity,
	case when DetailingSuppliesTurnovers.ReservedProduceQuantityTurnover -
		DetailingSuppliesTurnovers.WrittenOffProduceQuantityTurnover &gt; 0 then
	DetailingSuppliesTurnovers.ReservedProduceQuantityTurnover -
		DetailingSuppliesTurnovers.WrittenOffProduceQuantityTurnover 
		else 0 end AS ReservedProduceQuantity,
	DetailingSuppliesTurnovers.WrittenOffProduceQuantityTurnover AS WrittenOffProduceQuantity,
	
	
	(DetailingSuppliesTurnovers.EntryDemandQuantityTurnover + DetailingSuppliesTurnovers.CorrectedDemandQuantityTurnover)
	-(ISNULL(StockReservationBalance.QuantityBalance, 0) 
		+ DetailingSuppliesTurnovers.WrittenOffProduceQuantityTurnover 
		+ DetailingSuppliesTurnovers.ReservedProduceQuantityTurnover
	) 
	//- DetailingSuppliesTurnovers.NeededProduceQuantityTurnover 
	//+ DetailingSuppliesTurnovers.ProducedProduceQuantityTurnover
	AS NeededProcurementQuantity,
	
	DetailingSuppliesTurnovers.RequestProcurementQuantityTurnover AS RequestProcurementQuantity,
	
	((DetailingSuppliesTurnovers.EntryDemandQuantityTurnover + DetailingSuppliesTurnovers.CorrectedDemandQuantityTurnover)
	-(ISNULL(StockReservationBalance.QuantityBalance, 0) 
		+ DetailingSuppliesTurnovers.WrittenOffProduceQuantityTurnover 
		+ DetailingSuppliesTurnovers.ReservedProduceQuantityTurnover
	)) 
	- DetailingSuppliesTurnovers.RequestProcurementQuantityTurnover 
	//- DetailingSuppliesTurnovers.NeededProduceQuantityTurnover
	//+ DetailingSuppliesTurnovers.ProducedProduceQuantityTurnover
	AS LackProcurementQuantity,
	DetailingSuppliesTurnovers.OrderTransferQuantityTurnover AS OrderTransferQuantity,
	DetailingSuppliesTurnovers.ConfirmedTransferQuantityTurnover AS ConfirmedTransferQuantity,
	DetailingSuppliesTurnovers.OrderPurchaseQuantityTurnover AS OrderPurchaseQuantity,
	DetailingSuppliesTurnovers.ConfirmedPurchaseQuantityTurnover AS ConfirmedPurchaseQuantity
FROM
	AccumulationRegister.R7010T_DetailingSupplies.Turnovers AS DetailingSuppliesTurnovers
		LEFT JOIN AccumulationRegister.R4010B_ActualStocks.Balance AS StockBalanceBalance
		ON DetailingSuppliesTurnovers.Store = StockBalanceBalance.Store
		AND DetailingSuppliesTurnovers.ItemKey = StockBalanceBalance.ItemKey
		LEFT JOIN AccumulationRegister.R4012B_StockReservation.Balance AS StockReservationBalance
		ON DetailingSuppliesTurnovers.Store = StockReservationBalance.Store
		AND DetailingSuppliesTurnovers.ItemKey = StockReservationBalance.ItemKey</query>
	</dataSet>
