			<dataSource>DataSource1</dataSource>
			<query>SELECT
	T6020S_BatchKeysInfo.Company AS Company,
	T6020S_BatchKeysInfo.Store AS Store,
	T6020S_BatchKeysInfo.ItemKey AS ItemKey,
	VALUE(Enum.BatchType.NotCalculated) AS Batch,
	CASE
		WHEN T6020S_BatchKeysInfo.Direction = VALUE(Enum.BatchDirection.Receipt)
			THEN T6020S_BatchKeysInfo.Quantity
		ELSE 0
	END AS QuantityReceipt,
	CASE
		WHEN T6020S_BatchKeysInfo.Direction = VALUE(Enum.BatchDirection.Receipt)
			THEN T6020S_BatchKeysInfo.InvoiceAmount
		ELSE 0
	END AS InvoiceAmountReceipt,
	CASE
		WHEN T6020S_BatchKeysInfo.Direction = VALUE(Enum.BatchDirection.Expense)
			THEN T6020S_BatchKeysInfo.Quantity
		ELSE 0
	END AS QuantityExpense,
	case
		when T6020S_BatchKeysInfo.Recorder refs Document.ProductionCostsAllocation
			then T6020S_BatchKeysInfo.ProductionDocument
		when T6020S_BatchKeysInfo.Recorder refs Document.AdditionalCostAllocation
			then T6020S_BatchKeysInfo.PurchaseInvoiceDocument
		when T6020S_BatchKeysInfo.Recorder refs Document.AdditionalRevenueAllocation
			then T6020S_BatchKeysInfo.PurchaseInvoiceDocument
		else T6020S_BatchKeysInfo.Recorder
	end AS Recorder,
	T6020S_BatchKeysInfo.ItemKey.Item AS Item,
	FALSE AS IsRelevance
FROM
	InformationRegister.T6020S_BatchKeysInfo AS T6020S_BatchKeysInfo
		LEFT JOIN AccumulationRegister.R6020B_BatchBalance AS R6020B_BatchBalance
		ON T6020S_BatchKeysInfo.Company = R6020B_BatchBalance.Company
		AND T6020S_BatchKeysInfo.Store = R6020B_BatchBalance.Store
		AND T6020S_BatchKeysInfo.ItemKey = R6020B_BatchBalance.ItemKey
		AND case
			when T6020S_BatchKeysInfo.Recorder refs Document.ProductionCostsAllocation
				then T6020S_BatchKeysInfo.ProductionDocument
			when T6020S_BatchKeysInfo.Recorder refs Document.AdditionalCostAllocation
				then T6020S_BatchKeysInfo.PurchaseInvoiceDocument
			when T6020S_BatchKeysInfo.Recorder refs Document.AdditionalRevenueAllocation
				then T6020S_BatchKeysInfo.PurchaseInvoiceDocument
			else T6020S_BatchKeysInfo.Recorder
		end = R6020B_BatchBalance.Recorder
WHERE
	R6020B_BatchBalance.Recorder IS NULL
{WHERE
	case
		when T6020S_BatchKeysInfo.Recorder refs Document.ProductionCostsAllocation
			then T6020S_BatchKeysInfo.ProductionDocument.Date
		when T6020S_BatchKeysInfo.Recorder refs Document.AdditionalCostAllocation
			then T6020S_BatchKeysInfo.PurchaseInvoiceDocument.Date
		when T6020S_BatchKeysInfo.Recorder refs Document.AdditionalRevenueAllocation
			then T6020S_BatchKeysInfo.PurchaseInvoiceDocument.Date
		else T6020S_BatchKeysInfo.Period
	end BETWEEN BEGINOFPERIOD(&amp;BeginOfPeriod, DAY) AND ENDOFPERIOD(&amp;EndOfPeriod, DAY)}</query>
